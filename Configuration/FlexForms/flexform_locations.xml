<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <sheetTitle>LLL:EXT:jv_events/Resources/Private/Language/locallang_be.xlf:flexforms.Function
                </sheetTitle>
                <type>array</type>
                <el>
                    <settings.LayoutList>
                        <label>
                            LLL:EXT:jv_events/Resources/Private/Language/locallang_be.xlf:flexforms_additional.layout
                        </label>
                        <config>
                            <type>user</type>
                            <renderType>jvEventsCustomLayoutElement</renderType>
                            <size>30</size>
                            <userFunc>JVelletti\JvEvents\UserFunc\Flexforms->selectLayout</userFunc>
                            <parameters>
                                <layoutType>list.</layoutType>
                            </parameters>
                        </config>
                    </settings.LayoutList>
                    <settings.HideEventListLink>
                        <label>
                            LLL:EXT:jv_events/Resources/Private/Language/locallang_be.xlf:flexforms.HideEventListLink
                        </label>
                        <config>
                            <type>check</type>
                            <default>0</default>
                        </config>
                    </settings.HideEventListLink>
                </el>
            </ROOT>
        </sDEF>

        <sDEFloc>
            <ROOT>
                <sheetTitle>
                    LLL:EXT:jv_events/Resources/Private/Language/locallang_db.xlf:tx_jvevents_domain_model_location
                </sheetTitle>
                <type>array</type>
                <el>
                    <settings.location.new.defaultCity>
                        <label>Default City for new locations in Frontend editing</label>
                        <config>
                            <type>input</type>
                            <size>30</size>
                            <default>MÃ¼nchen</default>
                        </config>
                    </settings.location.new.defaultCity>
                    <settings.filterlocation.categories>
                        <label>LLL:EXT:jv_events/Resources/Private/Language/locallang_be.xlf:plugin.filter.Categories
                        </label>
                        <config>
                            <type>select</type>
                            <renderType>selectMultipleSideBySide</renderType>
                            <maxitems>100</maxitems>
                            <size>5</size>
                            <autoSizeMax>10</autoSizeMax>
                            <minitems>0</minitems>
                            <selectedListStyle>width:350px</selectedListStyle>
                            <itemListStyle>width:350px</itemListStyle>
                            <foreign_table>tx_jvevents_domain_model_category</foreign_table>
                            <foreign_table_where>AND tx_jvevents_domain_model_category.type = 1 AND
                                (tx_jvevents_domain_model_category.l10n_parent = 0 AND
                                tx_jvevents_domain_model_category.sys_language_uid in(-1, 0, CAST(
                                ###REC_FIELD_sys_language_uid### as SIGNED)) ) ORDER BY
                                tx_jvevents_domain_model_category.title
                            </foreign_table_where>
                            <allowNonIdValues>1</allowNonIdValues>
                        </config>
                    </settings.filterlocation.categories>
                    <settings.location.list.showMap>
                        <label>Show a Map</label>
                        <config>
                            <type>check</type>
                            <default>0</default>
                        </config>
                    </settings.location.list.showMap>
                    <settings.filterlocation.lng>
                        <label>Center Map with Longitude</label>
                        <displayCond>FIELD:sDEFloc.settings.location.list.showMap:REQ:true</displayCond>
                        <config>
                            <type>input</type>
                            <default>11.5806778</default>
                            <eval>float</eval>
                        </config>
                    </settings.filterlocation.lng>
                    <settings.filterlocation.lat>
                        <label>Center Map with Latitude</label>
                        <displayCond>FIELD:sDEFloc.settings.location.list.showMap:REQ:true</displayCond>
                        <config>
                            <type>input</type>
                            <default>48.1407067</default>
                            <eval>float</eval>
                        </config>
                    </settings.filterlocation.lat>
                    <settings.filterlocation.dist>
                        <label>max Distance in Kilometer</label>
                        <displayCond>FIELD:sDEFloc.settings.location.list.showMap:REQ:true</displayCond>
                        <config>
                            <type>number</type>
                            <default>100</default>
                        </config>
                    </settings.filterlocation.dist>
                    <settings.location.storePostion>
                        <label>Show option to store Map</label>
                        <displayCond>FIELD:sDEFloc.settings.location.list.showMap:REQ:true</displayCond>
                        <config>
                            <type>check</type>
                            <default>0</default>
                        </config>
                    </settings.location.storePostion>
                    <settings.filterorganizer.latestUpdate>
                        <label>
                            LLL:EXT:jv_events/Resources/Private/Language/locallang_be.xlf:plugin.filter.Organizer.latestUpdate
                        </label>
                        <config>
                            <type>number</type>
                            <default>0</default>
                        </config>
                    </settings.filterorganizer.latestUpdate>
                    <settings.filterlocation.hideList>
                        <label>
                            LLL:EXT:jv_events/Resources/Private/Language/locallang_be.xlf:plugin.filter.Locations.hideList
                        </label>
                        <config>
                            <type>check</type>
                            <default>0</default>
                        </config>
                    </settings.filterlocation.hideList>
                    <settings.filter.doubleclickzoom>
                        <label>Zoom factor for Map (1= world - 15 one street) when user Double clicks, default 10
                        </label>
                        <config>
                            <type>input</type>
                            <default>10</default>
                        </config>
                    </settings.filter.doubleclickzoom>
                </el>
            </ROOT>
        </sDEFloc>
        <sDEF5>
            <ROOT>
                <sheetTitle>LLL:EXT:jv_events/Resources/Private/Language/locallang_be.xlf:flexforms.DataSettings
                </sheetTitle>
                <type>array</type>
                <el>
                    <!-- PID Single Page -->
                    <settings.detailPid>
                        <label>
                            LLL:EXT:jv_events/Resources/Private/Language/locallang_be.xlf:flexforms_additional.detailPid
                        </label>
                        <config>
                            <type>group</type>
                            <internal_type>db</internal_type>
                            <allowed>pages</allowed>
                            <size>1</size>
                            <maxitems>1</maxitems>
                            <minitems>0</minitems>
                            <show_thumbs>1</show_thumbs>
                            <wizards>
                                <suggest>
                                    <type>suggest</type>
                                </suggest>
                            </wizards>
                        </config>
                    </settings.detailPid>
                    <!-- PID Single Page -->
                    <settings.pageIds.eventList>
                        <label>
                            LLL:EXT:jv_events/Resources/Private/Language/locallang_be.xlf:flexforms_additional.eventListPid
                        </label>
                        <config>
                            <type>group</type>
                            <internal_type>db</internal_type>
                            <allowed>pages</allowed>
                            <size>1</size>
                            <maxitems>1</maxitems>
                            <minitems>0</minitems>
                            <show_thumbs>1</show_thumbs>
                            <wizards>
                                <suggest>
                                    <type>suggest</type>
                                </suggest>
                            </wizards>
                        </config>
                    </settings.pageIds.eventList>
                    <settings.storagePid>
                        <exclude>1</exclude>
                        <label>LLL:EXT:lang/locallang_general.xml:LGL.startingpoint</label>
                        <config>
                            <type>group</type>
                            <internal_type>db</internal_type>
                            <allowed>pages</allowed>
                            <size>3</size>
                            <maxitems>50</maxitems>
                            <minitems>0</minitems>
                            <show_thumbs>1</show_thumbs>
                            <wizards>
                                <suggest>
                                    <type>suggest</type>
                                </suggest>
                            </wizards>
                        </config>
                    </settings.storagePid>
                    <settings.recursive>
                        <exclude>1</exclude>
                        <label>LLL:EXT:lang/locallang_general.xml:LGL.recursive</label>
                        <config>
                            <type>select</type>
                            <renderType>selectSingle</renderType>
                            <items type="array">
                                <numIndex index="1" type="array">
                                    <label>LLL:EXT:frontend/Resources/Private/Language/locallang_ttc.xlf:recursive.I.0
                                    </label>
                                    <value>0</value>
                                </numIndex>
                                <numIndex index="2" type="array">
                                    <label>LLL:EXT:frontend/Resources/Private/Language/locallang_ttc.xlf:recursive.I.1
                                    </label>
                                    <value>1</value>
                                </numIndex>
                                <numIndex index="3" type="array">
                                    <label>LLL:EXT:frontend/Resources/Private/Language/locallang_ttc.xlf:recursive.I.2
                                    </label>
                                    <value>2</value>
                                </numIndex>
                                <numIndex index="4" type="array">
                                    <label>LLL:EXT:frontend/Resources/Private/Language/locallang_ttc.xlf:recursive.I.3
                                    </label>
                                    <value>3</value>
                                </numIndex>
                                <numIndex index="5" type="array">
                                    <label>LLL:EXT:frontend/Resources/Private/Language/locallang_ttc.xlf:recursive.I.4
                                    </label>
                                    <value>4</value>
                                </numIndex>
                                <numIndex index="6" type="array">
                                    <label>LLL:EXT:frontend/Resources/Private/Language/locallang_ttc.xlf:recursive.I.5
                                    </label>
                                    <value>250</value>
                                </numIndex>
                            </items>
                        </config>
                    </settings.recursive>
                </el>
            </ROOT>
        </sDEF5>
    </sheets>
</T3DataStructure>
