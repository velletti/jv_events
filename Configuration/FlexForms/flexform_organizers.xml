                </TCEforms>
                <type>array</type>
                <el>
                    <settings.filterorganizer.hideInactives>
                        <TCEforms>
                            <label>LLL:EXT:jv_events/Resources/Private/Language/locallang_be.xlf:plugin.filter.Organizer.HideInactive</label>
                            <config>
                                <type>check</type>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </settings.filterorganizer.hideInactives>
                    <settings.filterorganizer.latestUpdate>
                        <TCEforms>
                            <label>LLL:EXT:jv_events/Resources/Private/Language/locallang_be.xlf:plugin.filter.Organizer.latestUpdate</label>
                            <config>
                                <type>input</type>
                                <eval>int</eval>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </settings.filterorganizer.latestUpdate>
                    <settings.filterorganizer.tags>
                        <TCEforms>
                            <label>LLL:EXT:jv_events/Resources/Private/Language/locallang_be.xlf:plugin.filterorganizer.Tags</label>
                            <config>
                                <type>select</type>
                                <renderType>selectMultipleSideBySide</renderType>

                                <maxitems>100</maxitems>
                                <size>5</size>
                                <autoSizeMax>10</autoSizeMax>
                                <minitems>0</minitems>
                                <selectedListStyle>width:350px</selectedListStyle>
                                <itemListStyle>width:350px</itemListStyle>
                                <foreign_table>tx_jvevents_domain_model_tag</foreign_table>
                                <foreign_table_where>  AND tx_jvevents_domain_model_tag.type = 2 AND (tx_jvevents_domain_model_tag.l10n_parent = 0 AND tx_jvevents_domain_model_tag.sys_language_uid in(-1, 0, CAST( ###REC_FIELD_sys_language_uid### as SIGNED)) ) ORDER BY tx_jvevents_domain_model_tag.name</foreign_table_where>
                                <allowNonIdValues>1</allowNonIdValues>
                            </config>
                        </TCEforms>
                    </settings.filterorganizer.tags>
                    <settings.filterorganizer.respectTagVisibility>
                        <TCEforms>
                            <label>LLL:EXT:jv_events/Resources/Private/Language/locallang_be.xlf:plugin.filter.Organizer.respectTagVisibility</label>
                            <config>
                                <type>check</type>
                                <default>1</default>
                            </config>
                        </TCEforms>
                    </settings.filterorganizer.respectTagVisibility>

                    <settings.filterorganizer.reverseSorting>
                        <TCEforms>
                            <label>LLL:EXT:jv_events/Resources/Private/Language/locallang_be.xlf:plugin.filter.Organizer.reverseSorting</label>
                            <config>
                                <type>check</type>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </settings.filterorganizer.reverseSorting>
                </el>
            </ROOT>
